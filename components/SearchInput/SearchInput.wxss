.searchHeader{
    width: 100% ;
    height: 80rpx;
    position: fixed;
    top: 9vh;
    padding: 10rpx 30rpx;
    display: flex;
    justify-content: space-between;
    box-sizing: border-box;
    background-color: skyblue;
    z-index: 999;
}
.input{
    width: 85%;
    height: 100%;
    background-color: #e8e5e5;
    border-radius: 30rpx;
    padding:0 60rpx;
    font-size: 28rpx;
}
.clear{
    width: 60rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    position: absolute;
    right: 120rpx;
    font-size: 45rpx;
}
.searchBtn{
    padding: 0;
    margin: 0;
    width: 100rpx;
    font-size: 28rpx;
    background-color: skyblue;
    line-height: 60rpx;
}
.searchBtn::after{
    border: none;
}
.icon-search{
    position: absolute;
    height: 60rpx;
    width: 60rpx;
    line-height: 60rpx;
    text-align: center;
    z-index: 9999;
}
